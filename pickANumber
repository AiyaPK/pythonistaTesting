#for pythonista

import random
#import ui

nameValDict = {"Tammy":3,"James":8,"Peter":1}

randPick = random.randint(0,10)
diffNamesDict={}
for nameVal in nameValDict:
  valDiff = abs(nameValDict[nameVal]-randPick)
  diffNamesDict[nameVal]=valDiff

for key,value in sorted(diffNamesDict.items(),key=lambda item:item[1]):
  print(f"{key}: \u0394{value}")

#print(randPick)

